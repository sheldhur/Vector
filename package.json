    "style-loader": "^0.19.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "antd": "^3.0.3",
    "binary-parser": "^1.3.0",
    "bluebird": "^3.5.1",
    "csv-string": "^3.1.2",
    "d3": "^4.12.2",
    "devtron": "^1.4.0",
    "dom-to-image": "^2.6.0",
    "electron-debug": "^1.4.0",
    "electron-log": "^2.2.13",
    "electron-redux": "^1.3.1",
    "electron-updater": "2.16.3",
