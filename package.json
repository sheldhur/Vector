{
  "name": "Vector",
  "productName": "Vector",
  "version": "2.0.0-beta.1",
  "description": "App for visualize Earth's geomagnetic structure in perioads of Solar activity by observatory and satelite data",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node ./test/runTests.js",
    "test-all": "npm run lint && npm run flow && npm run test && npm run build",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node ./test/runTests.js e2e",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node --max-old-space-size=8192 -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=8192 -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-worker": "cross-env NODE_ENV=production node --max-old-space-size=8192 -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.worker.js --progress --profile --colors",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\" \"npm run build-worker\"",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 npm run hot-server",
    "package": "npm run build && build --publish never",
    "package-release": "npm run build && build --publish always",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "clean": "node ./scripts/clean.js",
    "cleanup": "mop -v"
  },
  "browserslist": "electron 1.6.12",
  "build": {
    "productName": "Vector",
    "appId": "org.sheldhurMornor.Vector",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "assets/icons/",
      "assets/json/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "worker.js",
      "worker.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sheldhur/Vector.git"
  },
  "author": {
    "name": "Sheldhur Mornor",
    "email": "sheldhur@gmail.com",
    "url": "https://github.com/sheldhur"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sheldhur/Vector/issues"
  },
  "keywords": [
    "electron",
    "sqlite",
    "Earth",
    "Sun",
    "space",
    "weather",
    "geomagnetism",
    "intermagnet",
    "iaga2002",
    "cdaweb",
    "themis",
    "goes",
    "carisma"
  ],
  "homepage": "https://github.com/sheldhur/Vector#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "aws-sdk": "^2.102.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.5",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "~1.6.12",
    "electron-builder": "^19.24.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.6.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs-scripts": "^0.8.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.53.1",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jsdom": "^11.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "sinon": "^3.2.1",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "antd": "^2.12.7",
    "binary-parser": "^1.2.0",
    "bluebird": "^3.5.0",
    "csv-string": "^2.3.2",
    "d3": "^4.10.0",
    "deep-assign": "^2.0.0",
    "dom-to-image": "^2.5.2",
    "electron-debug": "^1.4.0",
    "electron-redux": "^1.3.0",
    "hex-to-rgb": "^1.0.1",
    "interpolating-polynomial": "^1.1.2",
    "moment": "^2.18.1",
    "natural-spline-interpolator": "^1.0.2",
    "react": "^15.6.1",
    "react-color": "^2.13.5",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.7.2",
    "redux-electron-store": "^0.4.1",
    "redux-ignore": "^1.2.4",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.7.0",
    "sequelize-cli": "^2.8.0",
    "sequelize-log-syntax-colors": "^1.0.0",
    "source-map-support": "^0.4.16",
    "sprintf-js": "^1.1.1",
    "sqlformatter": "^0.1.0",
    "sqlite3-offline": "^3.1.18",
    "topojson": "^3.0.0"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
